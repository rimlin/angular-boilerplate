{
  "name": "angular-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:prod:client && npm run build:prod:server && npm run server",
    "build": "webpack",
    "build:prod": "npm run build:prod:client && npm run build:prod:server",
    "build:prod:client": "webpack --env.aot --env.client --env.env production",
    "build:prod:server": "webpack --env.aot --env.server --env.env production",
    "build:stage": "npm run build:stage:client && npm run build:stage:server",
    "build:stage:client": "webpack --env.aot --env.client --env.env stage",
    "build:stage:server": "webpack --env.aot --env.server --env.env stage",
    "clean": "rimraf dist",
    "server:dev": "nodemon --inspect build/server.js  -L",
    "watch": "webpack --watch --env.env development",
    "debug:watch": "node --nolazy --inspect-brk=9222 ./node_modules/.bin/webpack --watch --env.env development",
    "forever:start": "forever start dist/server.js",
    "forever:stop": "forever stop dist/server.js",
    "deploy:prod": "npm run build:prod && npm run forever:stop && node ./scripts/build-prod.js && npm run forever:start",
    "ansible:deploy": "ansible-playbook -i scripts/ansible/production scripts/ansible/deploy.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimlin/angular-boilerplate.git"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "author": "rimlin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rimlin/angular-boilerplate/issues"
  },
  "homepage": "https://github.com/rimlin/angular-boilerplate#readme",
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/platform-server": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/entity": "github:ngrx/entity-builds",
    "@ngrx/router-store": "^4.0.4",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "@nguniversal/express-engine": "^1.0.0-beta.0",
    "body-parser": "^1.17.2",
    "bowser": "^1.7.1",
    "breakpoint-sass": "^2.7.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "flexboxgrid": "^6.3.1",
    "lodash": "^4.17.4",
    "ngrx-store-freeze": "^0.2.0",
    "ngx-cookie": "^1.0.0",
    "node-fetch": "^1.7.1",
    "normalize.css": "^7.0.0",
    "rxjs": "^5.x",
    "serve-favicon": "^2.4.3",
    "webpack-concat-svg-plugin": "^1.1.6",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.4.4",
    "@ngtools/webpack": "^1.2.14",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.8",
    "angular-router-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.2.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "enhanced-resolve": "^3.3.0",
    "forever": "^0.15.3",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "^0.17.0",
    "svg-inline-loader": "^0.7.1",
    "typescript": "^2.3.4",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-merge": "^4.0.0"
  }
}
